# ComfyUI Paths
COMFYUI_PATH=/path/to/ComfyUI
COMFYUI_ENV_PATH=/path/to/conda/env
MINIFORGE_PATH=/path/to/miniforge3

# Output Directories
OUTPUT_DIR=/path/to/outputs
TEMP_DIR=/path/to/temp
LOG_DIR=/path/to/logs
CACHE_DIR=/path/to/cache

# Network Settings
COMFYUI_HOST=0.0.0.0
COMFYUI_PORT=8188

# GPU Settings
CUDA_DEVICE=0
ENABLE_TF32=1
ENABLE_CUDNN_BENCHMARK=1
ENABLE_CUDA_MALLOC_ASYNC=1

# Performance
OMP_NUM_THREADS=auto
MKL_NUM_THREADS=auto

# UI Settings
THEME=dark
LANGUAGE=en
AUTO_UPDATE_CHECK=true